{
  "name": "@xad/api",
  "module": "index.ts",
  "type": "module",
  "private": true,
  "scripts": {
    "dev": "dotenvx run -f .env -- wrangler dev",
    "build": "echo 'No build step for API'",
    "start": "dotenvx run -- bun index.ts",
    "deploy": "wrangler deploy",
    "db:generate": "dotenvx run -- drizzle-kit generate",
    "db:migrate": "dotenvx run -- bun src/db/migrate.ts",
    "db:seed": "dotenvx run -- bun src/db/seed.ts"
  },
  "devDependencies": {
    "@types/bun": "latest",
    "drizzle-kit": "^0.31.4"
  },
  "peerDependencies": {
    "typescript": "^5"
  },
  "dependencies": {
    "@dotenvx/dotenvx": "^1.49.0",
    "@hono/node-server": "^1.19.2",
    "@hono/zod-validator": "^0.7.2",
    "@neondatabase/serverless": "^1.0.1",
    "@privy-io/server-auth": "^1.32.2",
    "@types/uuid": "^10.0.0",
    "drizzle-orm": "^0.44.5",
    "hono": "^4.9.4",
    "jose": "^6.1.0",
    "uuid": "^11.1.0",
    "viem": "^2.37.5",
    "wrangler": "^4.33.2",
    "zod": "^4.1.5"
  }
}
