{
  "name": "xad-request-extractor",
  "description": "Extract HTTP requests, GraphQL requests and responses from active tabs",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "wxt",
    "dev:firefox": "wxt -b firefox",
